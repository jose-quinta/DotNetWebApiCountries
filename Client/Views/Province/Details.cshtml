@model Client.Models.Province

<div class="row col-12 col-sm-10 col-md-9 col-lg-8 mx-auto">
    <h1 class="text-center my-3 fs-4">Delete Province</h1>
    <div class="form-group row mt-1">
        <div class="col-4">
            <label asp-for="Name" class="form-label"></label>
        </div>
        <div class="col-8">
            <input asp-for="Name" readonly class="form-control" />
        </div>
    </div>
    <div class="form-group row mt-1">
        <div class="col-4">
            <label asp-for="Country" class="form-label"></label>
        </div>
        <div class="col-8">
            <input asp-for="CountryId" readonly class="form-control" />
        </div>
    </div>
    <div class="form-group row mt-3">
        <div class="form-group row col-12 col-sm-10 col-md-9 col-lg-8 mx-auto">
            <div class="col-6">
                <a asp-controller="Province" asp-action="Edit" asp-route-Id="@Model.Id" class="btn btn-primary form-control">Edit</a>
            </div>
            <div class="col-6">
                <a asp-controller="Province" asp-action="Index" class="btn btn-secondary form-control">Return to List</a>
            </div>
        </div>
    </div>
</div>